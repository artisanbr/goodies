{
    "name": "artisanbr/goodies",
    "description": "Laravel/PHP goodies pack by Artisan Digital",
    "type": "package",
    "version": "v0.0.4",
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^10.0|^11.0|^12.0",
        "illuminate/view": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "ArtisanBR\\Goodies\\": "src/"
        },
        "files": [
            "src/Helpers/helpers.php"
        ]
    },
    "authors": [
        {
            "name": "Renalcio Jr."
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "laravel/framework": "^10.0|^11.0|^12.0",
        "laravel/tinker": "^2.10"
    },
    "extra": {
        "laravel": {
            "providers": [
                "ArtisanBR\\Goodies\\GoodiesServiceProvider"
            ]
        }
    }
}
